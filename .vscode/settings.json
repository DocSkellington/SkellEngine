        "SKELLENGINE_BUILD_DOCUMENTATION": "TRUE",
        "SKELLENGINE_BUILD_TESTS": "TRUE",
        "SKELLENGINE_BUILD_EXAMPLES": "TRUE",
        "CMAKE_INSTALL_PREFIX": "${workspaceFolder}/bin"
